#include<stdio.h>
#include<string.h>
int main()
{
	char input[30];
	int len;
	printf("Enter a line of text:");
	scanf("%[^\n]s",input);
	if(input[0]=='/' && input[1]=='/')
{
	printf("single line comment");
}
  else if (input[0]=='/'&& input[1]=='*')
{
  printf("multiple comment");
}
else
{
  printf("not a comment");
}
  return 0;
}
